{
  "name": "static-react",
  "version": "1.0.0",
  "description": "React static site generator",
  "author": "Brent Jackson",
  "license": "MIT",
  "main": "build.js",
  "peerDependencies": {
    "react": "^0.12.2",
    "react-router": "^0.12.4"
  },
  "dependencies": {
    "lodash": "^3.5.0",
    "node-jsx": "^0.12.4"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "http-server": "^0.7.5",
    "parallelshell": "^1.1.1",
    "react-html": "0.0.4",
    "react-tools": "^0.12.2",
    "reactify": "^1.0.0",
    "watch": "^0.14.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "node ./examples/build.js",
    "watch:example": "watch 'npm run example' ./examples",
    "serve": "http-server -p 8000",
    "start": "parallelshell 'npm run watch:example' 'npm run serve'"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jxnblk/static-react.git"
  },
  "bugs": {
    "url": "https://github.com/jxnblk/static-react/issues"
  },
  "homepage": "https://github.com/jxnblk/static-react"
}
